{
  "name": "ingenious-stream-youtube",
  "version": "1.0.0",
  "description": "A curated collection of YouTube-hosted movie metadata for streaming applications",
  "main": "index.js",
  "scripts": {
    "fetch": "node scripts/fetch-tamil-movies.js",
    "fetch-tamil-playlist": "node 00-extract/scripts/tamil/fetch-playlist.js",
    "fetch-tamil-keywords": "node 00-extract/scripts/tamil/fetch-keywords.js",
    "fetch-tamil-video-details": "node 00-extract/scripts/tamil/fetch-playlist-video-details.js",
    "fetch-telugu-playlist": "node 00-extract/scripts/telugu/fetch-playlist.js",
    "fetch-telugu-video-details": "node 00-extract/scripts/telugu/fetch-playlist-video-details.js",
    "transform-tamil": "node 02-load/scripts/filter-tamil.js",
    "cleanup-tamil-playlist": "node 01-transform/scripts/cleanup-tamil-playlist.js",
    "cleanup-tamil-playlist-01": "node 01-transform/scripts/cleanup-tamil-playlist-01.js",
    "cleanup-telugu-playlist": "node 01-transform/scripts/cleanup-telugu-playlist.js",
    "filter-tamil": "node 02-load/scripts/filter-tamil.js",
    "filter-telugu": "node 02-load/scripts/filter-telugu.js",
    "group-tamil": "node 02-load/scripts/group-tamil-movies.js",
    "group-telugu": "node 02-load/scripts/group-telugu-movies.js",
    "extract": "npm run fetch-tamil-playlist && npm run fetch-tamil-keywords && npm run fetch-telugu-playlist",
    "transform": "npm run transform-tamil && npm run cleanup-tamil-playlist && npm run cleanup-tamil-playlist-01 && npm run cleanup-telugu-playlist",
    "load": "npm run filter-tamil && npm run group-tamil && npm run filter-telugu && npm run group-telugu",
    "pipeline": "npm run extract && npm run transform && npm run load",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "youtube",
    "movies",
    "tamil",
    "telugu",
    "metadata",
    "streaming"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "puppeteer": "^24.34.0"
  }
}
